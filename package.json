{
  "name": "own-store-mini-app-rn",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "npm run sync:contract && expo start",
    "eject": "expo eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "sync:contract": "rm -rf ./contract && cp -fR ../own-store-api/src/app/contract .",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "tailwind:build": "tailwindcss --input styles/input.css --output styles/tailwind.css --no-autoprefixer && tailwind-rn -i styles/tailwind.css -o styles/tailwind.json",
    "tailwind:dev": "concurrently \"tailwindcss --input styles/input.css --output styles/tailwind.css --no-autoprefixer --watch\" \"tailwind-rn -i styles/tailwind.css -o styles/tailwind.json --watch\""
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.3.0",
    "debug": "^4.3.3",
    "dotenv": "^14.3.2",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.4",
    "expo-constants": "^13.0.1",
    "expo-device": "~4.1.0",
    "expo-font": "~10.0.4",
    "expo-splash-screen": "~0.14.0",
    "expo-status-bar": "~1.2.0",
    "expo-web-browser": "~10.1.0",
    "fetch-retry": "^5.0.1",
    "postcss": "^8.4.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "tailwind-rn": "^4.0.0",
    "react-native-svg": "12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/debug": "^4.1.7",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "concurrently": "^7.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.1",
    "tailwindcss": "^3.0.17",
    "typescript": "~4.3.5"
  }
}
